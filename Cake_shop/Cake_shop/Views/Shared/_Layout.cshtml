    <script src="~/assets/js/product-dt.js"></script>
    <script src="~/assets/js/tab.js"></script>
    <script src="~/assets/js/ConvertPrice.js"></script>
    <script src="~/assets/js/authen.js"></script>
    <script src="~/assets/js/quantity.js"></script>
    <script src="~/Scripts/jShopping.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>

    <script>
    var isLoggedIn = @(Session["userID"] != null ? "true" : "false");

    if (isLoggedIn === "false" && window.location.pathname !== '@Url.Action("DangNhap", "Home", new { area = "Admin" })') {
        window.location.href = '@Url.Action("DangNhap", "Home", new { area = "Admin" })';
    }
    </script>


