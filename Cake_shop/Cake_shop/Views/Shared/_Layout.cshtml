<body>
    <!-- Header + Navigation -->
    @Html.Partial("_Header")

    <div class="app__container">
        <div id="gap-30129" class="gapelement" style="display: block; height: auto;">
            <style>
                #gap-30129 {
                    padding-top: 15px;
                }
            </style>
        </div>
        <div class="grid-2">
            <div class="grid__row">
                @Html.Action("MenuProducts", "Category")
                <!-- slide show -->
                @RenderBody()
            </div>
        </div>
    </div>

    @Html.Partial("_Footer")

    <script src="~/assets/js/indexx.js"></script>
